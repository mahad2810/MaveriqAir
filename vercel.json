{
  "builds": [
    { "src": "vite.config.ts", "use": "@vercel/vite" },
    { "src": "api/**/*.ts", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/map.html", "dest": "/map.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "outputDirectory": "dist"
}
