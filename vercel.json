{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.js" },
    { "source": "/(assets|images|css|js)/(.*)", "destination": "/$1/$2" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
} 